}\n\n.d3-tip {\n  line-height: 1;\n  font-weight: bold;\n  background: rgba(0, 0, 0, 0.8);\n  color: #fff;\n  border-radius: 2px;\n  border: solid 1px #fff;\n  width: 332px;\n  padding: 10px;\n}\n\n.d3-tip h1 {\n  font-size: 20px;\n  text-align: center;\n}\n\ndl {\n  display: flex;\n  flex-wrap: wrap;\n}\n\ndt {\n  display: block;\n  width: 150px;\n  text-align: right;\n  padding-right: 10px;\n  padding-bottom: 2px;\n}\n\ndd {\n  width: 150px;\n  display: block;\n  white-space: nowrap;\n  padding-bottom: 2px;\n  margin-inline-start: 0;\n}\n\ntable,\nth,\ntd {\n  border: 1px solid black;\n}\n\n.legendTitle {\n  fill: white;\n  font: 16px sans-serif;\n  font-weight: bold;\n}\n\n.label {\n  fill: white;\n  font-size: 16px;\n}\n\n@media (max-width: 1100px) {\n  #stellerTempLegend .legendCells .label {\n    font-size: 12px;\n  }\n}\n\n@media (max-width: 800px) {\n  #stellerTempLegend .legendCells .label {\n    font-size: 10px;\n  }\n}\n\n@media (max-width: 700px) {\n  #stellerTempLegend .legendCells .label {\n    font-size: 8px;\n  }\n}\n\n@media (max-width: 400px) {\n  #stellerTempLegend .legendCells .label {\n    font-size: 6px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"c373592455fa9971efe29c50ee90932e","cacheData":{"env":{}}}